{
  "datasets": [
    {
      "name": "b6c3a5a1",
      "displayName": "hourly_weather_dataset",
      "queryLines": [
        "select\n",
        "  station_id,\n",
        "  station_name,\n",
        "  observation_timestamp,\n",
        "  text_description,\n",
        "  temperature_value,\n",
        "  temperature_unit,\n",
        "  wind_speed_value,\n",
        "  wind_speed_unit,\n",
        "  wind_direction_value,\n",
        "  wind_direction_unit,\n",
        "  visibility_value,\n",
        "  visibility_unit,\n",
        "  max_temperature_last_24_hours_value,\n",
        "  max_temperature_last_24_hours_unit,\n",
        "  min_temperature_last_24_hours_value,\n",
        "  min_temperature_last_24_hours_unit\n",
        "from workspace.default.prod_hourly_weather_report\n",
        "where observation_timestamp >= current_timestamp() - INTERVAL 30 days"
      ]
    }
  ],
  "pages": [
    {
      "name": "c2a2d243",
      "displayName": "Daily Trend",
      "layout": [
        {
          "widget": {
            "name": "b402b696",
            "multilineTextboxSpec": {
              "lines": [
                "## <span style=\"text-align:center;display:block\">HOURLY WEATHER REPORT</span>"
              ]
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 6,
            "height": 1
          }
        },
        {
          "widget": {
            "name": "2f41d690",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "b6c3a5a1",
                  "fields": [
                    {
                      "name": "station_name",
                      "expression": "`station_name`"
                    },
                    {
                      "name": "hourly(observation_timestamp)",
                      "expression": "DATE_TRUNC(\"HOUR\", `observation_timestamp`)"
                    },
                    {
                      "name": "temperature_value",
                      "expression": "`temperature_value`"
                    }
                  ],
                  "disaggregated": true
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "hourly(observation_timestamp)",
                  "displayName": "Date/Hour",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fieldName": "temperature_value",
                  "displayName": "Temperature",
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "color": {
                  "fieldName": "station_name",
                  "legend": {
                    "position": "bottom"
                  },
                  "displayName": "Station",
                  "scale": {
                    "type": "categorical"
                  }
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Temperature Per Hour"
              }
            }
          },
          "position": {
            "x": 0,
