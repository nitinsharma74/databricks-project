{
  "datasets": [
    {
      "name": "b6c3a5a1",
      "displayName": "hourly_weather_dataset",
      "queryLines": [
        "select\n",
        "  station_id,\n",
        "  station_name,\n",
        "  observation_timestamp,\n",
        "  text_description,\n",
        "  temperature_value,\n",
        "  temperature_unit,\n",
        "  wind_speed_value,\n",
        "  wind_speed_unit,\n",
        "  wind_direction_value,\n",
        "  wind_direction_unit,\n",
        "  visibility_value,\n",
        "  visibility_unit,\n",
        "  max_temperature_last_24_hours_value,\n",
        "  max_temperature_last_24_hours_unit,\n",
        "  min_temperature_last_24_hours_value,\n",
        "  min_temperature_last_24_hours_unit\n",
        "from workspace.default.hourly_weather_report\n",
        "where observation_timestamp >= current_timestamp() - INTERVAL 72 HOURS"
      ]
    }
  ],
